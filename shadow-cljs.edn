;; shadow-cljs configuration
{:deps {:aliases [:cljs]}
 :nrepl {:middleware [refactor-nrepl.middleware/wrap-refactor]}
 :builds
 {:app {:target :browser
        :output-dir "public/js"
        :asset-path "/js"
        :modules {:main {:entries [aws-amplify-cljs.core]}}
        :devtools {:http-root "public"
                   :http-port 8080}}}}
